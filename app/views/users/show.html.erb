<div class="center">
  Welcome, <%= @user.email %> <br>
  You joined on: <%= @user.created_at.strftime("%b %d, %Y") %>

<div class="">
  <ul classs>
  <% if current_user %>
  <%= link_to "Profile", user_path(current_user), class: "btn btn-md btn-primary" %>
  <%= link_to "Log Out", logout_path, class: "btn btn-md btn-danger" %>
  <% end %>
  </ul>
</div>


</div>
